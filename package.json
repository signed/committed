{
  "name": "ts-starter",
  "version": "1.0.0",
  "description": "Basic typescript setup for tdd",
  "packageManager": "pnpm@8.6.6",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "execute": "tsx src/index.mts",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "format": "prettier --write ."
  },
  "dependencies": {
    "dotenv": "16.3.1",
    "simple-git": "3.19.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "1.0.2",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.4.0",
    "prettier": "3.0.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vitest": "0.33.0"
  },
  "prettier": "@signed/prettier-config"
}
