{
  "name": "committed",
  "version": "1.0.0",
  "description": "Identify the tickets mentioned in commit messages since the last release",
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "dev": "pnpm clean && pnpm run server",
    "prod": "pnpm clean && pnpm run lint && pnpm run build && pnpm run server:prod",
    "build": "pnpm typecheck && vite build",
    "typecheck": "tsc --noEmit",
    "server": "tsx ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production tsx ./server/index.ts",
    "lint": "eslint . --max-warnings 0",
    "clean": "rimraf ./dist",
    "format": "prettier --write .",
    "execute": "tsx src/index.ts",
    "nitpick": "pnpm format && pnpm lint && pnpm test && pnpm typecheck",
    "demo-repo": "tsx ./scripts/create-example-repository.ts"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/material": "7.3.4",
    "@trpc/client": "11.7.1",
    "@trpc/server": "11.7.1",
    "compression": "1.8.1",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "simple-git": "3.28.0",
    "sirv": "3.0.2",
    "vike": "0.4.235",
    "vite": "7.1.12",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@signed/prettier-config": "0.2.0",
    "@tsconfig/node22": "22.0.2",
    "@tsconfig/strictest": "2.0.6",
    "@types/compression": "1.8.1",
    "@types/express": "5.0.5",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitejs/plugin-react": "5.1.0",
    "cross-env": "10.1.0",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "random-words": "2.0.1",
    "rimraf": "6.0.1",
