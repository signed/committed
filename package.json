{
  "name": "committed",
  "version": "1.0.0",
  "description": "Identify the tickets mentioned in commit messages since the last release",
  "packageManager": "pnpm@9.15.4+sha256.9bee59c7313a216722c079c1e22160dea7f88df4e0c3450b1d7b01b882336c6a",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "dev": "pnpm clean && pnpm run server",
    "prod": "pnpm clean && pnpm run lint && pnpm run build && pnpm run server:prod",
    "build": "pnpm typecheck && vite build",
    "typecheck": "tsc --noEmit",
    "server": "tsx ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production tsx ./server/index.ts",
    "lint": "eslint . --max-warnings 0",
    "clean": "rimraf ./dist",
    "format": "prettier --write .",
    "execute": "tsx src/index.ts",
    "nitpick": "pnpm format && pnpm lint && pnpm test && pnpm typecheck",
    "demo-repo": "tsx ./scripts/create-example-repository.ts"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/material": "6.4.0",
    "@trpc/client": "10.45.2",
    "@trpc/server": "10.45.2",
    "compression": "1.7.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "simple-git": "3.27.0",
    "sirv": "3.0.0",
    "vike": "0.4.214",
    "vite": "6.0.7",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@signed/prettier-config": "0.2.0",
    "@tsconfig/node20": "20.1.4",
    "@tsconfig/strictest": "2.0.5",
    "@types/compression": "1.7.5",
    "@types/express": "5.0.0",
    "@types/node": "22.10.6",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "@vitejs/plugin-react": "4.3.4",
    "cross-env": "7.0.3",
    "eslint": "8.51.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "prettier": "3.4.2",
    "random-words": "2.0.1",
    "rimraf": "6.0.1",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "vitest": "2.1.8"
  },
  "prettier": "@signed/prettier-config"
}
