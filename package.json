{
  "name": "committed",
  "version": "1.0.0",
  "description": "Identify the tickets mentioned in commit messages since the last release",
  "packageManager": "pnpm@10.11.0+sha256.a69e9cb077da419d47d18f1dd52e207245b29cac6e076acedbeb8be3b1a67bd7",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "dev": "pnpm clean && pnpm run server",
    "prod": "pnpm clean && pnpm run lint && pnpm run build && pnpm run server:prod",
    "build": "pnpm typecheck && vite build",
    "typecheck": "tsc --noEmit",
    "server": "tsx ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production tsx ./server/index.ts",
    "lint": "eslint . --max-warnings 0",
    "clean": "rimraf ./dist",
    "format": "prettier --write .",
    "execute": "tsx src/index.ts",
    "nitpick": "pnpm format && pnpm lint && pnpm test && pnpm typecheck",
    "demo-repo": "tsx ./scripts/create-example-repository.ts"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/material": "7.1.2",
    "@trpc/client": "11.4.2",
    "@trpc/server": "11.4.2",
    "compression": "1.8.0",
    "dotenv": "16.6.0",
    "express": "4.21.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "simple-git": "3.28.0",
    "sirv": "3.0.1",
    "vike": "0.4.235",
    "vite": "7.0.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@signed/prettier-config": "0.2.0",
    "@tsconfig/node20": "20.1.6",
    "@tsconfig/strictest": "2.0.5",
    "@types/compression": "1.8.1",
    "@types/express": "5.0.3",
    "@types/node": "24.0.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "@vitejs/plugin-react": "4.6.0",
    "cross-env": "7.0.3",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.2.0",
    "prettier": "3.6.1",
    "random-words": "2.0.1",
    "rimraf": "6.0.1",
