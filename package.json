{
  "name": "committed",
  "version": "1.0.0",
  "description": "Identify the tickets mentioned in commit messages since the last release",
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "dev": "pnpm clean && pnpm run server",
    "prod": "pnpm clean && pnpm run lint && pnpm run build && pnpm run server:prod",
    "build": "pnpm typecheck && vite build",
    "typecheck": "tsc --noEmit",
    "server": "tsx ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production tsx ./server/index.ts",
    "lint": "eslint . --max-warnings 0",
    "clean": "rimraf ./dist",
    "format": "prettier --write .",
    "execute": "tsx src/index.ts",
    "nitpick": "pnpm format && pnpm lint && pnpm test && pnpm typecheck",
    "demo-repo": "tsx ./scripts/create-example-repository.ts"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/material": "7.3.6",
    "@trpc/client": "11.7.2",
    "@trpc/server": "11.7.2",
    "compression": "1.8.1",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "simple-git": "3.30.0",
    "sirv": "3.0.2",
    "vike": "0.4.235",
    "vite": "7.2.6",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@signed/prettier-config": "0.2.0",
    "@tsconfig/node22": "22.0.5",
    "@tsconfig/strictest": "2.0.8",
    "@types/compression": "1.8.1",
    "@types/express": "5.0.6",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "@vitejs/plugin-react": "5.1.1",
    "cross-env": "10.1.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "random-words": "2.0.1",
    "rimraf": "6.1.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1",
