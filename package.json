{
  "name": "ts-starter",
  "version": "1.0.0",
  "description": "Basic typescript setup for tdd",
  "packageManager": "pnpm@8.6.6",
  "private": true,
  "scripts": {
    "execute": "tsx ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@tsconfig/node20": "1.0.2",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.4.0",
    "prettier": "3.0.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  },
  "prettier": "@signed/prettier-config"
}
