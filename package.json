{
  "name": "committed",
  "version": "1.0.0",
  "description": "Identify the tickets mentioned in commit messages since the last release",
  "packageManager": "pnpm@9.12.2",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "dev": "pnpm clean && pnpm run server",
    "prod": "pnpm clean && pnpm run lint && pnpm run build && pnpm run server:prod",
    "build": "pnpm typecheck && vite build",
    "typecheck": "tsc --noEmit",
    "server": "tsx ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production tsx ./server/index.ts",
    "lint": "eslint . --max-warnings 0",
    "clean": "rimraf ./dist",
    "format": "prettier --write .",
    "execute": "tsx src/index.ts",
    "nitpick": "pnpm format && pnpm lint && pnpm test && pnpm typecheck",
    "demo-repo": "tsx ./scripts/create-example-repository.ts"
  },
  "dependencies": {
    "@trpc/client": "10.45.2",
    "@trpc/server": "10.45.2",
    "compression": "1.7.4",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "simple-git": "3.27.0",
    "sirv": "3.0.0",
    "vike": "0.4.199",
    "vite": "5.4.10",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@signed/prettier-config": "0.2.0",
    "@tsconfig/node20": "20.1.4",
    "@tsconfig/strictest": "2.0.5",
    "@types/compression": "1.7.5",
    "@types/express": "4.17.20",
    "@types/node": "22.7.9",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "@vitejs/plugin-react": "4.3.3",
    "cross-env": "7.0.3",
    "eslint": "8.51.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "prettier": "3.3.3",
    "random-words": "2.0.1",
    "rimraf": "6.0.1",
    "tsx": "4.19.1",
    "typescript": "5.6.3",
    "vitest": "2.1.3"
  },
  "prettier": "@signed/prettier-config"
}
